#include<stdio.h>
int main(){
    int x,d=0,num=0,original; 
    printf("Enter a number: \n");
    scanf("%d",&x);
    original=x;
    while(x>0){
        d=x%10;
        num=(num*10)+d;
        x=x/10;
    }
    if(original==num){
        printf("Its a palindrome");
    }
    else{
        printf("It isnt a palindrome");
    }
    return 0;
}
